<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="swim.swimmom" >

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name=".MeetActivity"
            android:label="@string/title_activity_meet" >
        </activity>
        <activity
            android:name=".CutTimeActivity"
            android:label="@string/title_activity_cut_time" >
        </activity>
        <activity
            android:name=".StatisticActivity"
            android:label="@string/title_activity_statistic" >
        </activity>
        <activity
            android:name=".ProfileAddActivity"
            android:label="@string/title_activity_profile_add" >
        </activity>
        <activity
            android:name=".ProfileEditActivity"
            android:label="@string/title_activity_profile_edit" >
        </activity>
        <activity
            android:name=".ProfileDeleteActivity"
            android:label="@string/title_activity_profile_delete" >
        </activity>
        <activity
            android:name=".MeetCreateActivity"
            android:label="@string/title_activity_meet_create" >
        </activity>
    </application>

</manifest>
